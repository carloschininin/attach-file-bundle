{
    "name": "carloschininin/attach-file-bundle",
    "description": "Upload files",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Carlos Chininin",
            "email": "carloschininin@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.3|^4.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.11|^3.0",
        "symfony/cache": "^6.0|^7.0",
        "symfony/http-foundation": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "CarlosChininin\\AttachFile\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.35"
    },
    "suggest": {
        "ext-gd": "PHP GD image library",
        "imagine/imagine": "Image library",
        "symfony/form": "Symfony forms component"
    }
}
